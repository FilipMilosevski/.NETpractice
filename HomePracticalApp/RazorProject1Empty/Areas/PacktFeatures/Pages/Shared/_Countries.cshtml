@model Filip.Country
<div class="card border-dark mb-3" style="max-width: 18rem;">
    <div class="card-header">@Model?.CountryCode2, @Model?.CountryCode3</div>
    <div class="card-body text-dark">
        <h5 class="card-title">@Model?.NationalDay</h5>
        <p class="card-text">@Model?.Name</p>
    </div>
</div>